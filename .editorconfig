# EditorConfig for Azure On-Prem Lift & Shift Lab
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.sh]
indent_size = 2
indent_style = space
charset = utf-8
end_of_line = lf

[*.md]
trim_trailing_whitespace = false

[*.json]
indent_size = 2
indent_style = space

[*.yml]
indent_size = 2
indent_style = space

[*.yaml]
indent_size = 2


